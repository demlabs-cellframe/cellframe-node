<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Required for Python interpreter to work with JIT compilation -->
    <key>com.apple.security.cs.allow-jit</key>
    <true/>
    
    <!-- Required for loading Python extension modules (.so files) -->
    <key>com.apple.security.cs.allow-unsigned-executable-memory</key>
    <true/>
    
    <!-- Required for loading Python plugins that may not be signed with same team ID -->
    <key>com.apple.security.cs.disable-library-validation</key>
    <true/>
    
    <!-- Allow dyld environment variables for Python path configuration -->
    <key>com.apple.security.cs.allow-dyld-environment-variables</key>
    <true/>
</dict>
</plist>


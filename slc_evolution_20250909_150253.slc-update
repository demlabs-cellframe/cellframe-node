      "modules/projects/chipmunk_structure_module.json": "{\n  \"version\": \"1.0.0\",\n  \"created\": \"2025-01-28T12:00:00Z\",\n  \"updated\": \"2025-01-28T12:00:00Z\",\n  \"navigation_system\": {\n    \"purpose\": \"–ú–æ–¥—É–ª—å —Å—Ç—Ä—É–∫—Ç—É—Ä—ã Chipmunk –ø–æ–¥–ø–∏—Å–∏ –¥–ª—è –ø–æ–≤—Ç–æ—Ä–Ω–æ–≥–æ –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏—è\",\n    \"recovery_path\": \".context/manifest.json\",\n    \"current_file\": \".context/modules/projects/chipmunk_structure_module.json\",\n    \"file_role\": \"CHIPMUNK_STRUCTURE_MODULE\",\n    \"related_files\": [\n      \".context/manifest.json\",\n      \".context/modules/projects/cryptography_project.json\",\n      \".context/modules/projects/dap_sdk_project.json\"\n    ],\n    \"quick_navigation\": {\n      \"üè† –ì–ª–∞–≤–Ω–∞—è\": \".context/manifest.json - –≥–ª–∞–≤–Ω—ã–π –Ω–∞–≤–∏–≥–∞—Ç–æ—Ä —Å–∏—Å—Ç–µ–º—ã\",\n      \"üîê Chipmunk\": \".context/modules/projects/chipmunk_structure_module.json - —Å—Ç—Ä—É–∫—Ç—É—Ä–∞ –ø–æ–¥–ø–∏—Å–∏\",\n      \"üõ†Ô∏è CLI\": \".context/tools/scripts/slc_cli.py - –∞–≤—Ç–æ–º–∞—Ç–∏–∑–∞—Ü–∏—è\"\n    },\n    \"usage_hint\": \"./slc info modules/projects/chipmunk_structure_module.json –¥–ª—è –ø–æ–¥—Ä–æ–±–Ω–æ–π –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏\",\n    \"ai_context\": \"–°–õ–ö –º–æ–¥—É–ª—å —Å—Ç—Ä—É–∫—Ç—É—Ä—ã Chipmunk –ø–æ–¥–ø–∏—Å–∏ –¥–ª—è –ø–æ–≤—Ç–æ—Ä–Ω–æ–≥–æ –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏—è –≤ ChipmunkRing\"\n  },\n  \"chipmunk_structure\": {\n    \"algorithm_overview\": {\n      \"name\": \"Chipmunk\",\n      \"type\": \"Lattice-based digital signature scheme\",\n      \"basis\": \"Module Learning With Errors (MLWE) problem\",\n      \"security\": \"Quantum-resistant\",\n      \"research_paper\": \"https://eprint.iacr.org/2023/1820\"\n    },\n    \"core_parameters\": {\n      \"CHIPMUNK_N\": {\n        \"value\": 512,\n        \"description\": \"Ring dimension (polynomial degree)\",\n        \"purpose\": \"–û–ø—Ä–µ–¥–µ–ª—è–µ—Ç —Ä–∞–∑–º–µ—Ä –∫–æ–ª—å—Ü–∞ –¥–ª—è –ø–æ–ª–∏–Ω–æ–º–∏–∞–ª—å–Ω—ã—Ö –æ–ø–µ—Ä–∞—Ü–∏–π\"\n      },\n      \"CHIPMUNK_SEC_PARAM\": {\n        \"value\": 112,\n        \"description\": \"Security parameter in bits\",\n        \"purpose\": \"–ì–∞—Ä–∞–Ω—Ç–∏—Ä—É–µ—Ç –∫—Ä–∏–ø—Ç–æ–≥—Ä–∞—Ñ–∏—á–µ—Å–∫—É—é —Å—Ç–æ–π–∫–æ—Å—Ç—å\"\n      },\n      \"CHIPMUNK_HEIGHT\": {\n        \"value\": 16,\n        \"description\": \"Height of the tree\",\n        \"purpose\": \"–ü–æ–¥–¥–µ—Ä–∂–∏–≤–∞–µ—Ç –¥–æ 32,768 —É—á–∞—Å—Ç–Ω–∏–∫–æ–≤ –≤ –º—É–ª—å—Ç–∏-–ø–æ–¥–ø–∏—Å–∏\"\n      },\n      \"CHIPMUNK_Q\": {\n        \"value\": 3168257,\n        \"description\": \"HOTS modulus\",\n        \"purpose\": \"–û—Å–Ω–æ–≤–Ω–æ–π –º–æ–¥—É–ª—å –¥–ª—è –∞—Ä–∏—Ñ–º–µ—Ç–∏–∫–∏ –≤ –∫–æ–ª—å—Ü–µ\"\n      }\n    },\n    \"key_structures\": {\n      \"public_key\": {\n        \"size_bytes\": \"32 + CHIPMUNK_N*4*2\",\n        \"components\": {\n          \"rho_seed\": {\n            \"size\": \"32 bytes\",\n            \"purpose\": \"Seed for generating matrix A parameters\"\n          },\n          \"v0\": {\n            \"type\": \"chipmunk_poly_t\",\n            \"purpose\": \"Public key polynomial v0 = Œ£(a[i] * s0[i])\"\n          },\n          \"v1\": {\n            \"type\": \"chipmunk_poly_t\",\n            \"purpose\": \"Public key polynomial v1 = Œ£(a[i] * s1[i])\"\n          }\n        }\n      },\n      \"private_key\": {\n        \"size_bytes\": \"32 + 48 + CHIPMUNK_PUBLIC_KEY_SIZE\",\n        \"components\": {\n          \"key_seed\": {\n            \"size\": \"32 bytes\",\n            \"purpose\": \"Master seed for generating s0[i] and s1[i]\"\n          },\n          \"tr\": {\n            \"size\": \"48 bytes\",\n            \"purpose\": \"Public key commitment (SHA3-384 hash)\"\n          },\n          \"pk\": {\n            \"type\": \"chipmunk_public_key_t\",\n            \"purpose\": \"Embedded public key\"\n          }\n        }\n      },\n      \"signature\": {\n        \"size_bytes\": \"CHIPMUNK_N*4*CHIPMUNK_GAMMA\",\n        \"components\": {\n          \"sigma\": {\n            \"type\": \"chipmunk_poly_t[CHIPMUNK_GAMMA]\",\n            \"purpose\": \"HOTS signature polynomials\"\n          }\n        }\n      }\n    },\n    \"mathematical_components\": {\n      \"polynomial_operations\": {\n        \"ntt\": {\n          \"description\": \"Number Theoretic Transform\",\n          \"purpose\": \"–≠—Ñ—Ñ–µ–∫—Ç–∏–≤–Ω–æ–µ —É–º–Ω–æ–∂–µ–Ω–∏–µ –ø–æ–ª–∏–Ω–æ–º–æ–≤ –≤ –∫–æ–ª—å—Ü–µ\"\n        },\n        \"invntt\": {\n          \"description\": \"Inverse NTT\",\n          \"purpose\": \"–û–±—Ä–∞—Ç–Ω–æ–µ –ø—Ä–µ–æ–±—Ä–∞–∑–æ–≤–∞–Ω–∏–µ –¥–ª—è –≤–µ—Ä–∏—Ñ–∏–∫–∞—Ü–∏–∏\"\n        },\n        \"sampling\": {\n          \"description\": \"–ü–æ–ª–∏–Ω–æ–º–∏–∞–ª—å–Ω–æ–µ —Å—ç–º–ø–ª–∏—Ä–æ–≤–∞–Ω–∏–µ\",\n          \"types\": [\"uniform\", \"gaussian\", \"binary\"]\n        }\n      },\n      \"lattice_operations\": {\n        \"matrix_a_generation\": {\n          \"description\": \"–ì–µ–Ω–µ—Ä–∞—Ü–∏—è –º–∞—Ç—Ä–∏—Ü—ã A –∏–∑ seed\",\n          \"purpose\": \"–°–æ–∑–¥–∞–Ω–∏–µ –æ—Ç–∫—Ä—ã—Ç–æ–π –º–∞—Ç—Ä–∏—Ü—ã –¥–ª—è MLWE\"\n        },\n        \"decomposition\": {\n          \"description\": \"–†–∞–∑–ª–æ–∂–µ–Ω–∏–µ –∫–æ—ç—Ñ—Ñ–∏—Ü–∏–µ–Ω—Ç–æ–≤\",\n          \"purpose\": \"–£–º–µ–Ω—å—à–µ–Ω–∏–µ —Ä–∞–∑–º–µ—Ä–∞ —Å–∏–≥–Ω–∞—Ç—É—Ä—ã\"\n        },\n        \"aggregation\": {\n          \"description\": \"–ê–≥—Ä–µ–≥–∞—Ü–∏—è –º—É–ª—å—Ç–∏-–ø–æ–¥–ø–∏—Å–µ–π\",\n          \"purpose\": \"–ö–æ–º–±–∏–Ω–∏—Ä–æ–≤–∞–Ω–∏–µ –ø–æ–¥–ø–∏—Å–µ–π –Ω–µ—Å–∫–æ–ª—å–∫–∏—Ö —É—á–∞—Å—Ç–Ω–∏–∫–æ–≤\"\n        }\n      },\n      \"hash_functions\": {\n        \"primary\": \"SHA3-384 –¥–ª—è commitment\",\n        \"secondary\": \"SHA2-256 –¥–ª—è –æ–±—â–µ–≥–æ —Ö—ç—à–∏—Ä–æ–≤–∞–Ω–∏—è\",\n        \"specialized\": \"Chipmunk-specific hash functions\"\n      }\n    },\n    \"security_properties\": {\n      \"quantum_resistance\": {\n        \"basis\": \"MLWE problem hardness\",\n        \"assumptions\": [\"LWE\", \"Ring-LWE\", \"Module-LWE\"],\n        \"attack_resistance\": [\"Shor's algorithm\", \"Grover's algorithm\"]\n      },\n      \"classical_security\": {\n        \"collision_resistance\": \"SHA3-based\",\n        \"preimage_resistance\": \"SHA3-based\",\n        \"second_preimage_resistance\": \"SHA3-based\"\n      },\n      \"implementation_security\": {\n        \"constant_time\": \"–í—Å–µ –æ–ø–µ—Ä–∞—Ü–∏–∏ –ø–æ—Å—Ç–æ—è–Ω–Ω–æ–≥–æ –≤—Ä–µ–º–µ–Ω–∏\",\n        \"memory_safety\": \"–ó–∞—â–∏—â–µ–Ω–Ω–∞—è —Ä–∞–±–æ—Ç–∞ —Å –ø–∞–º—è—Ç—å—é\",\n        \"side_channel_resistance\": \"–ó–∞—â–∏—Ç–∞ –æ—Ç –ø–æ–±–æ—á–Ω—ã—Ö –∫–∞–Ω–∞–ª–æ–≤\"\n      }\n    },\n    \"performance_characteristics\": {\n      \"key_sizes\": {\n        \"public_key\": \"~4KB\",\n        \"private_key\": \"~8KB\",\n        \"signature\": \"~8KB\"\n      },\n      \"operation_times\": {\n        \"key_generation\": \"~310ms\",\n        \"signing\": \"~33ms\",\n        \"verification\": \"~15ms\"\n      },\n      \"optimization_opportunities\": {\n        \"simd\": \"AVX2/NEON vectorization\",\n        \"batch_processing\": \"–ü–∞–∫–µ—Ç–Ω–∞—è –æ–±—Ä–∞–±–æ—Ç–∫–∞ –ø–æ–¥–ø–∏—Å–µ–π\",\n        \"memory_hierarchy\": \"–û–ø—Ç–∏–º–∏–∑–∞—Ü–∏—è –∫—ç—à–∞\"\n      }\n    },\n    \"integration_points\": {\n      \"dap_sdk\": {\n        \"location\": \"module/crypto/src/chipmunk/\",\n        \"api_file\": \"chipmunk.h\",\n        \"implementation\": \"chipmunk.c\",\n        \"tests\": \"module/crypto/test/crypto/dap_enc_chipmunk_test.c\"\n      },\n      \"cellframe_blockchain\": {\n        \"usage\": \"–ú—É–ª—å—Ç–∏-–ø–æ–¥–ø–∏—Å–∏ –¥–ª—è –∫–æ–Ω—Å–µ–Ω—Å—É—Å–∞\",\n        \"scalability\": \"–ü–æ–¥–¥–µ—Ä–∂–∫–∞ —Ç—ã—Å—è—á —É—á–∞—Å—Ç–Ω–∏–∫–æ–≤\",\n        \"performance\": \"–û–ø—Ç–∏–º–∏–∑–∏—Ä–æ–≤–∞–Ω–æ –¥–ª—è –±–ª–æ–∫—á–µ–π–Ω-—Ç—Ä–∞–Ω–∑–∞–∫—Ü–∏–π\"\n      }\n    },\n    \"reusability_patterns\": {\n      \"core_components\": {\n        \"polynomial_arithmetic\": \"–ë–∞–∑–æ–≤—ã–µ –æ–ø–µ—Ä–∞—Ü–∏–∏ —Å –ø–æ–ª–∏–Ω–æ–º–∞–º–∏\",\n        \"sampling_functions\": \"–ì–µ–Ω–µ—Ä–∞—Ü–∏—è —Å–ª—É—á–∞–π–Ω—ã—Ö –ø–æ–ª–∏–Ω–æ–º–æ–≤\",\n        \"hash_abstractions\": \"–ê–±—Å—Ç—Ä–∞–∫—
      "modules/core/development_standards.json": "{\n  \"title\": \"–û–±—â–∏–µ —Å—Ç–∞–Ω–¥–∞—Ä—Ç—ã —Ä–∞–∑—Ä–∞–±–æ—Ç–∫–∏\",\n  \"description\": \"–°—Ç–∞–Ω–¥–∞—Ä—Ç—ã –∏ —à–∞–±–ª–æ–Ω—ã –¥–ª—è —Ä–∞–∑—Ä–∞–±–æ—Ç–∫–∏ –ø—Ä–æ–µ–∫—Ç–æ–≤\",\n  \"category\": \"core\",\n  \"type\": \"standards\",\n  \"version\": \"1.0.0\",\n  \"created\": \"2025-01-21\",\n  \"updated\": \"2025-01-21\",\n  \n  \"ci_cd_standards\": {\n    \"description\": \"–°—Ç–∞–Ω–¥–∞—Ä—Ç–Ω—ã–µ —à–∞–±–ª–æ–Ω—ã –¥–ª—è CI/CD –∫–æ–Ω—Ñ–∏–≥—É—Ä–∞—Ü–∏–π\",\n    \"gitlab_ci_template\": {\n      \"description\": \"–ë–∞–∑–æ–≤—ã–π —à–∞–±–ª–æ–Ω –¥–ª—è GitLab CI/CD —Ñ–∞–π–ª–æ–≤\",\n      \"template\": \".build:\\n  tags:\\n    - ci-polygon\\n  image: demlabs/debian/amd64:qt5\\n  timeout: 3 hours 30 minutes\\n  dependencies: []\\n  interruptible: true\",\n      \"usage\": \"–ò—Å–ø–æ–ª—å–∑–æ–≤–∞—Ç—å –∫–∞–∫ –æ—Å–Ω–æ–≤—É –¥–ª—è –≤—Å–µ—Ö .gitlab-ci.yml —Ñ–∞–π–ª–æ–≤\",\n      \"mandatory_fields\": [\n        \"tags: - ci-polygon\",\n        \"image: demlabs/debian/amd64:qt5\", \n        \"timeout: 3 hours 30 minutes\",\n        \"dependencies: []\",\n        \"interruptible: true\"\n      ],\n      \"explanation\": \"–≠—Ç–æ—Ç —à–∞–±–ª–æ–Ω –æ–±–µ—Å–ø–µ—á–∏–≤–∞–µ—Ç —Å—Ç–∞–Ω–¥–∞—Ä—Ç–Ω—É—é –∫–æ–Ω—Ñ–∏–≥—É—Ä–∞—Ü–∏—é –¥–ª—è –≤—Å–µ—Ö CI/CD pipeline –≤ –æ—Ä–≥–∞–Ω–∏–∑–∞—Ü–∏–∏\"\n    },\n    \"docker_standards\": {\n      \"base_image\": \"demlabs/debian/amd64:qt5\",\n      \"timeout_policy\": \"3 hours 30 minutes –º–∞–∫—Å–∏–º—É–º –¥–ª—è –ª—é–±–æ–π —Å–±–æ—Ä–∫–∏\",\n      \"interruptible_policy\": \"–í—Å–µ –¥–∂–æ–±—ã –¥–æ–ª–∂–Ω—ã –±—ã—Ç—å interruptible: true\",\n      \"dependencies_policy\": \"–Ø–≤–Ω–æ —É–∫–∞–∑—ã–≤–∞—Ç—å dependencies: [] –µ—Å–ª–∏ –Ω–µ—Ç –∑–∞–≤–∏—Å–∏–º–æ—Å—Ç–µ–π\"\n    }\n  },\n  \n  \"python_binding_rules\": {\n    \"description\": \"–ö—Ä–∏—Ç–∏—á–µ—Å–∫–∏ –≤–∞–∂–Ω—ã–µ –ø—Ä–∞–≤–∏–ª–∞ –¥–ª—è Python –±–∏–Ω–¥–∏–Ω–≥–æ–≤\",\n    \"never_simplify_rule\": {\n      \"rule\": \"–ö–†–ò–¢–ò–ß–ï–°–ö–ò –í–ê–ñ–ù–û: –ù–ï –£–ü–†–û–©–ê–¢–¨ Pyth
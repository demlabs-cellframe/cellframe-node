      "modules/tools/intelligent_recommendation_engine.json": "{\n  \"name\": \"–ò–Ω—Ç–µ–ª–ª–µ–∫—Ç—É–∞–ª—å–Ω–∞—è —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ç–µ–ª—å–Ω–∞—è —Å–∏—Å—Ç–µ–º–∞\",\n  \"version\": \"1.0.0\",\n  \"category\": \"tools\",\n  \"description\": \"TF-IDF –∞–ª–≥–æ—Ä–∏—Ç–º –∏ —Å–µ–º–∞–Ω—Ç–∏—á–µ—Å–∫–∏–π –∞–Ω–∞–ª–∏–∑ –¥–ª—è —É–º–Ω—ã—Ö —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–π –º–æ–¥—É–ª–µ–π –°–õ–ö\",\n  \"created\": \"2025-01-14\",\n  \"author\": \"–°–∞–º–æ—Ä–∞–∑–≤–∏—Ç–∏–µ –°–õ–ö - –§–∞–∑–∞ 2 —Ä–µ–∞–ª–∏–∑–∞—Ü–∏–∏\",\n  \"tags\": [\n    \"tf-idf\",\n    \"—Å–µ–º–∞–Ω—Ç–∏—á–µ—Å–∫–∏–π-–∞–Ω–∞–ª–∏–∑\",\n    \"—Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–∏\",\n    \"nlp\",\n    \"–º–∞—à–∏–Ω–Ω–æ–µ-–æ–±—É—á–µ–Ω–∏–µ\"\n  ],\n  \"metadata\": {\n    \"complexity\": \"—Å—Ä–µ–¥–Ω—è—è\",\n    \"–≤—Ä–µ–º—è_—Ä–µ–∞–ª–∏–∑–∞—Ü–∏–∏\": \"2-3 –¥–Ω—è\",\n    \"—Ü–µ–ª–µ–≤–∞—è_–∞—É–¥–∏—Ç–æ—Ä–∏—è\": \"—Å–∏—Å—Ç–µ–º–∞_—Å–ª–∫\",\n    \"—Ç—Ä–µ–±–æ–≤–∞–Ω–∏—è\": [\n      \"math\",\n      \"collections\",\n      \"re\",\n      \"context_usage_tracker\"\n    ]\n  },\n  \"–Ω–∞–∑–Ω–∞—á–µ–Ω–∏–µ\": {\n    \"–æ–ø–∏—Å–∞–Ω–∏–µ\": \"–ê–Ω–∞–ª–∏–∑ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—å—Å–∫–∏—Ö –∑–∞–ø—Ä–æ—Å–æ–≤ –∏ –ø—Ä–µ–¥–æ—Å—Ç–∞–≤–ª–µ–Ω–∏–µ –∏–Ω—Ç–µ–ª–ª–µ–∫—Ç—É–∞–ª—å–Ω—ã—Ö —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–π –Ω–∞ –æ—Å–Ω–æ–≤–µ TF-IDF –∏ –∏—Å—Ç–æ—Ä–∏–∏ –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏—è\",\n    \"–∏–Ω—Ç–µ–≥—Ä–∞—Ü–∏—è_—Å\": [\n      \"modules/tools/context_usage_tracker.json\",\n      \"modules/methodologies/intelligent_context_prediction.json\"\n    ],\n    \"–≤—ã—Ö–æ–¥–Ω—ã–µ_–¥–∞–Ω–Ω—ã–µ\": \"–†–∞–Ω–∂–∏—Ä–æ–≤–∞–Ω–Ω—ã–π —Å–ø–∏—Å–æ–∫ —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–π —Å –æ—Ü–µ–Ω–∫–∞–º–∏ —Ä–µ–ª–µ–≤–∞–Ω—Ç–Ω–æ—Å—Ç–∏\"\n  },\n  \"–∞–ª–≥–æ—Ä–∏—Ç–º_tf_idf\": {\n    \"–æ–ø–∏—Å–∞–Ω–∏–µ\": \"Term Frequency - Inverse Document Frequency –¥–ª—è –∞–Ω–∞–ª–∏–∑–∞ —Ä–µ–ª–µ–≤–∞–Ω—Ç–Ω–æ—Å—Ç–∏ –º–æ–¥—É–ª–µ–π\",\n    \"–∫–æ–º–ø–æ–Ω–µ–Ω—Ç—ã\": {\n      \"term_frequency\": {\n        \"—Ñ–æ—Ä–º—É–ª–∞\": \"TF(t,d) = (–∫–æ–ª–∏—á–µ—Å—Ç–≤–æ_–≤—Ö–æ–∂–¥–µ–Ω–∏–π_—Ç–µ—Ä–º–∏–Ω–∞_t_–≤_–¥–æ–∫—É–º–µ–Ω—Ç–µ_d) / (–æ–±—â–µ–µ_–∫–æ–ª–∏—á–µ—Å—Ç–≤–æ_—Ç–µ—Ä–º–∏–Ω–æ–≤_–≤_–¥–æ–∫—É–º–µ–Ω—Ç–µ_d)\",\n        \"–ø—Ä–∏–º–µ–Ω–µ–Ω–∏–µ\": \"–ê–Ω–∞–ª–∏–∑ —á–∞—Å—Ç–æ—Ç—ã –∫–ª—é—á–µ–≤—ã—Ö —Å–ª–æ–≤ –≤ –∑–∞–ø—Ä–æ—Å–µ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è\"\n      },\n      \"inverse_document_frequency\": {\n        \"—Ñ–æ—Ä–º—É–ª–∞\": \"IDF(t,D) = log(|D| / |{d ‚àà D : t ‚àà d}|)\",\n        \"–ø—Ä–∏–º–µ–Ω–µ–Ω–∏–µ\": \"–û–ø—Ä–µ–¥–µ–ª–µ–Ω–∏–µ —É–Ω–∏–∫–∞–ª—å–Ω–æ—Å—Ç–∏ —Ç–µ—Ä–º–∏–Ω–∞ –≤ –∫–æ–ª–ª–µ–∫—Ü–∏–∏ –º–æ–¥—É–ª–µ–π\"\n      },\n      \"tf_idf_score\": {\n        \"—Ñ–æ—Ä–º—É–ª–∞\": \"TF-IDF(t,d,D) = TF(t,d) * IDF(t,D)\",\n        \"–ø—Ä–∏–º–µ–Ω–µ–Ω–∏–µ\": \"–§–∏–Ω–∞–ª—å–Ω–∞—è –æ—Ü–µ–Ω–∫–∞ —Ä–µ–ª–µ–≤–∞–Ω—Ç–Ω–æ—Å—Ç–∏ –º–æ–¥—É–ª—è –¥–ª—è –∑–∞–ø—Ä–æ—Å–∞\"\n      }\n    },\n    \"–¥–æ–∫—É–º–µ–Ω—Ç–Ω–∞—è_–∫–æ–ª–ª–µ–∫—Ü–∏—è\": {\n      \"–∏—Å—Ç–æ—á–Ω–∏–∫–∏\": [\n        \"–ù–∞–∑–≤–∞–Ω–∏—è –º–æ–¥—É–ª–µ–π –∏ —à–∞–±–ª–æ–Ω–æ–≤\",\n        \"–û–ø–∏—Å–∞–Ω–∏—è –º–æ–¥—É–ª–µ–π\",\n        \"–ö–ª—é—á–µ–≤—ã–µ —Å–ª–æ–≤–∞ –∏ —Ç–µ–≥–∏\",\n        \"Target projects —Å–ø–∏—Å–∫–∏\",\n        \"–ò—Å—Ç–æ—Ä–∏—è –ø–æ–∏—Å–∫–æ–≤—ã—Ö –∑–∞–ø—Ä–æ—Å–æ–≤\"\n      ],\n      \"–ø—Ä–µ–¥–æ–±—Ä–∞–±–æ—Ç–∫–∞_—Ç–µ–∫—Å—Ç–∞\": {\n        \"—Ç–æ–∫–µ–Ω–∏–∑–∞—Ü–∏—è\": \"–†–∞–∑–±–∏–µ–Ω–∏–µ –Ω–∞ –æ—Ç–¥–µ–ª—å–Ω—ã–µ —Å–ª–æ–≤–∞\",\n        \"–Ω–æ—Ä–º–∞–ª–∏–∑–∞—Ü–∏—è\": \"–ü—Ä–∏–≤–µ–¥–µ–Ω–∏–µ –∫ –Ω–∏–∂–Ω–µ–º—É —Ä–µ–≥–∏—Å—Ç—Ä—É\",\n        \"—É–¥–∞–ª–µ–Ω–∏–µ_—Å—Ç–æ–ø_—Å–ª–æ–≤\": [\n          \"–∏\",\n          \"–∏–ª–∏\",\n          \"–≤\",\n          \"–Ω–∞\",\n          \"–¥–ª—è\",\n          \"—Å\",\n          \"a\",\n          \"an\",\n          \"the\",\n          \"in\",\n          \"on\",\n          \"for\"\n        ],\n        \"—Å—Ç–µ–º–º–∏–Ω–≥\": \"–£–ø—Ä–æ—â–µ–Ω–Ω–æ–µ –ø—Ä–∏–≤–µ–¥–µ–Ω–∏–µ –∫ –æ—Å–Ω–æ–≤–µ —Å–ª–æ–≤–∞\"\n      }\n    }\n  },\n  \"—Å–∏—Å—Ç–µ–º–∞_–≤–µ—Å–æ–≤\": {\n    \"–æ–ø–∏—Å–∞–Ω–∏–µ\": \"–ö–æ–º–±–∏–Ω–∏—Ä–æ–≤–∞–Ω–∏–µ —Ä–∞–∑–ª–∏—á–Ω—ã—Ö —Ñ–∞–∫—Ç–æ—Ä–æ–≤ –¥–ª—è —Ñ–∏–Ω–∞–ª—å–Ω–æ–π –æ—Ü–µ–Ω–∫–∏\",\n    \"–≤–µ—Å–æ–≤—ã–µ_–∫–æ—ç—Ñ—Ñ–∏—Ü–∏–µ–Ω—Ç—ã\": {\n      \"tf_idf_score\": {\n        \"–≤–µ—Å\": 0.4,\n        \"–æ–ø–∏—Å–∞–Ω–∏–µ\": \"–°–µ–º–∞–Ω—Ç–∏—á–µ—Å–∫–∞—è —Ä–µ–ª–µ–≤–∞–Ω—Ç–Ω–æ—Å—Ç—å –Ω–∞ –æ—Å–Ω–æ–≤–µ TF-IDF\"\n      },\n      \"usage_frequency\": {\n        \"–≤–µ—Å\": 0.3,\n        \"–æ–ø–∏—Å–∞–Ω–∏–µ\": \"–ß–∞—Å—Ç–æ—Ç–∞ –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏—è –º–æ–¥—É–ª—è –≤ –∏—Å—Ç–æ—Ä–∏–∏\"\n      },\n      \"contextual_relevance\": {\n        \"–≤–µ—Å\": 0.2,\n        \"–æ–ø–∏—Å–∞–Ω–∏–µ\": \"–°–æ–æ—Ç–≤–µ—Ç—Å—Ç–≤–∏–µ –∫–æ–Ω—Ç–µ–∫—Å—Ç—É —Ç–µ–∫—É—â–µ–π —Ä–∞–±–æ—Ç—ã\"\n      },\n      \"user_feedback\": {\n        \"–≤–µ—Å\": 0.1,\n        \"–æ–ø–∏—Å–∞–Ω–∏–µ\": \"–û–±—Ä–∞—Ç–Ω–∞—è —Å–≤—è–∑—å –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è (–µ—Å–ª–∏ –¥–æ—Å—Ç—É–ø–Ω–∞)\"\n      }\n    },\n    \"–Ω–æ—Ä–º–∞–ª–∏–∑–∞—Ü–∏—è\": {\n      \"–º–µ—Ç–æ–¥\": \"min-max_scaling\",\n      \"–¥–∏–∞–ø–∞–∑–æ–Ω\": \"[0, 1] –¥–ª—è –∫–∞–∂–¥–æ–≥–æ –∫–æ–º–ø–æ–Ω–µ–Ω—Ç–∞\",\n      \"—Ñ–æ—Ä–º—É–ª–∞\": \"normalized = (value - min) / (max - min)\"\n    },\n    \"—Ñ–∏–Ω–∞–ª—å–Ω–∞—è_—Ñ–æ—Ä–º—É–ª–∞\": \"final_score = Œ£(component_i * weight_i) –¥–ª—è –≤—Å–µ—Ö –∫–æ–º–ø–æ–Ω–µ–Ω—Ç–æ–≤\"\n  },\n  \"—Å–µ–º–∞–Ω—Ç–∏—á–µ—Å–∫–∏–π_–∞–Ω–∞–ª–∏–∑\": {\n    \"–∞–Ω–∞–ª–∏–∑_–Ω–∞–º–µ—Ä–µ–Ω–∏–π\": {\n      \"–æ–ø–∏—Å–∞–Ω–∏–µ\": \"–û–ø—Ä–µ–¥–µ–ª–µ–Ω–∏–µ —Ç–∏–ø–∞ –∑–∞–ø—Ä–æ—Å–∞ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è\",\n      \"–∫–∞—Ç–µ–≥–æ—Ä–∏–∏_–Ω–∞–º–µ—Ä–µ–Ω–∏–π\": {\n        \"–ø–æ–∏—Å–∫_—à–∞–±–ª–æ–Ω–∞\": {\n          \"–ø–∞—Ç—Ç–µ—Ä–Ω—ã\": [\n            \"—Å–æ–∑–¥–∞—Ç—å\",\n            \"—à–∞–±–ª–æ–Ω\",\n            \"template\",\n            \"–Ω–æ–≤—ã–π –ø—Ä–æ–µ–∫—Ç\"\n          ],\n          \"–¥–µ–π—Å—Ç–≤–∏–µ\": \"–ü—Ä–∏–æ—Ä–∏—Ç–µ—Ç –º–æ–¥—É–ª—è–º —Å –≥–æ—Ç–æ–≤—ã–º–∏ —à–∞–±–ª–æ–Ω–∞–º–∏\"\n        },\n        \"–∏–∑—É—á–µ–Ω–∏–µ_–∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏\": {\n          \"–ø–∞—Ç—Ç–µ—Ä–Ω—ã\": [\n            \"—á—Ç–æ —Ç–∞–∫–æ–µ\",\n            \"–∫–∞–∫\",\n            \"–¥–æ–∫—É–º–µ–Ω—Ç–∞—Ü–∏—è\",\n            \"info\"\n          ],\n          \"–¥–µ–π—Å—Ç–≤–∏–µ\": \"–ü—Ä–∏–æ—Ä–∏—Ç–µ—Ç –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–æ–Ω–Ω—ã–º –∏ –º–µ—Ç–æ–¥–æ–ª–æ–≥–∏—á–µ—Å–∫–∏–º –º–æ–¥—É–ª—è–º\"\n        },\n        \"—Ä–µ—à–µ–Ω–∏–µ_–ø—Ä–æ–±–ª–µ–º—ã\": {\n          \"–ø–∞—Ç—Ç–µ—Ä–Ω—ã\": [\n            \"–æ—à–∏–±–∫–∞\",\n            \"–ø—Ä–æ–±–ª–µ–º–∞\",\n            \"fix\",\n            \"debug\",\n            \"–ø–æ–º–æ—â—å\"\n          ],\n          \"–¥–µ–π—Å—Ç–≤–∏–µ\": \"–ü—Ä–∏–æ—Ä–∏—Ç–µ—Ç tools –∏ troubleshooting –º–æ–¥—É–ª—è–º\"\n        },\n        \"–∞–Ω–∞–ª–∏–∑_—Ç–µ—Ö–Ω–æ–ª–æ–≥–∏–∏\": {\n          \"–ø–∞—Ç—Ç–µ—Ä–Ω—ã\": [\n            \"–∞–Ω–∞–ª–∏–∑\",\n            \"audit\",\n            \"review\",\n            \"–æ—Ü–µ–Ω–∫–∞\"\n          ],\n          \"–¥–µ–π—Å—Ç–≤–∞–Ω–∏–µ\": \"–ü—Ä–∏–æ—Ä–∏—Ç–µ—Ç –∞–Ω–∞–ª–∏—Ç–∏—á–µ—Å–∫–∏–º –º–µ—Ç–æ–¥–æ–ª–æ–≥–∏—è–º\"\n        }\n      }\n    },\n    \"–∫–æ–Ω—Ç–µ–∫—Å—Ç—É–∞–ª—å–Ω—ã–µ_–ø–æ–¥—Å–∫–∞–∑–∫–∏\": {\n      \"–≤—Ä–µ–º–µ–Ω–Ω–æ–π_–∫–æ–Ω—Ç–µ–∫—Å—Ç\": \"–ê–Ω–∞–ª–∏–∑ –Ω–µ–¥–∞–≤–Ω–∏—Ö –¥–µ–π—Å—Ç–≤–∏–π –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è\",\n      \"—Ç–µ–º–∞—Ç–∏—á–µ—Å–∫–∏–π_–∫–æ–Ω—Ç–µ–∫—Å—Ç\": \"–û–ø—Ä–µ–¥–µ–ª–µ–Ω–∏–µ –æ–±–ª–∞—Å—Ç–∏ —Ä–∞–±–æ—Ç—ã (crypto, web, mobile)\",\n      \"—É—Ä–æ–≤–µ–Ω—å_—Å–ª–æ–∂–Ω–æ—Å—Ç–∏\": \"–ü–æ–¥–±–æ—Ä –º–æ–¥—É–ª–µ–π —Å–æ–æ—Ç–≤–µ—Ç—Å—Ç–≤—É—é—â–µ–π —Å–ª–æ–∂–Ω–æ—Å—Ç–∏\"\n    }\n  },\n  \"—Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ç–µ–ª—å–Ω—ã–π_–¥–≤–∏–∂–æ–∫\": {\n    \"–∞–ª–≥–æ—Ä–∏—Ç–º_—Ä–∞–±–æ—Ç—ã\": [\n      \"1. –ü—Ä–µ–¥–æ–±—Ä–∞–±–æ—Ç–∫–∞ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—å—Å–∫–æ–≥–æ –∑–∞–ø—Ä–æ—Å–∞\",\n      \"2. –ê–Ω–∞–ª–∏–∑ –Ω–∞–º–µ—Ä–µ–Ω–∏–π –∏ –∏–∑–≤–ª–µ—á–µ–Ω–∏–µ –∫–ª—é—á–µ–≤—ã—Ö —Ç–µ—Ä–º–∏–Ω–æ–≤\",\n      \"3. –†–∞—Å—á–µ—Ç TF-IDF scores –¥–ª—è –≤—Å–µ—Ö –º–æ–¥—É–ª–µ–π\",\n      \"4. –ü–æ–ª—É—á–µ–Ω–∏–µ —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∏ –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏—è –∏–∑ UsageTracker\",\n      \"5. –ê–Ω–∞–ª–∏–∑ –∫–æ–Ω—Ç–µ–∫—Å—Ç—É–∞–ª—å–Ω–æ–π —Ä–µ–ª–µ–≤–∞–Ω—Ç–Ω–æ—Å—Ç–∏\",\n      \"6. –ö–æ–º–±–∏–Ω–∏—Ä–æ–≤–∞–Ω–∏–µ –≤—Å–µ—Ö —Ñ–∞–∫—Ç–æ—Ä–æ–≤ —Å –≤–µ—Å–æ–≤—ã–º–∏ –∫–æ—ç—Ñ—Ñ–∏—Ü–∏–µ–Ω—Ç–∞–º–∏\",\n      \"7. –†–∞–Ω–∂–∏—Ä–æ–≤–∞–Ω–∏–µ —Ä–µ–∑—É–ª—å—Ç–∞—Ç–æ–≤ –ø–æ —Ñ–∏–Ω–∞–ª—å–Ω—ã–º –æ—Ü–µ–Ω–∫–∞–º\",\n      \"8. –§–∏–ª—å—Ç—Ä–∞—Ü–∏—è –∏ –≤–æ–∑–≤—Ä–∞—Ç —Ç–æ–ø-N —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–π\"\n    ],\n    \"–æ–ø—Ç–∏–º–∏–∑–∞—Ü–∏–∏\": {\n      \"–∫—ç—à–∏—Ä–æ–≤–∞–Ω–∏–µ\": \"TF-IDF –≤–µ–∫—Ç–æ—Ä—ã –º–æ–¥—É–ª–µ–π –∫—ç—à–∏—Ä—É—é—Ç—Å—è –¥–æ –∏–∑–º–µ–Ω–µ–Ω–∏—è\",\n      \"–ª–µ–Ω–∏–≤—ã–µ_–≤—ã—á–∏—Å–ª–µ–Ω–∏—è\": \"IDF –ø–µ—Ä–µ—Å—á–∏—Ç—ã–≤–∞–µ—Ç—Å—è —Ç–æ–ª—å–∫–æ –ø—Ä–∏ –¥–æ–±–∞–≤–ª–µ–Ω–∏–∏ –Ω–æ–≤—ã—Ö –º–æ–¥—É–ª–µ–π\",\n      \"–ø–∞–∫–µ—Ç–Ω–∞—è_–æ–±—Ä–∞–±–æ—Ç–∫–∞\": \"–û–±—Ä–∞–±–æ—Ç–∫–∞ –º–Ω–æ–∂–µ—Å—Ç–≤–µ–Ω–Ω—ã—Ö –∑–∞–ø—Ä–æ—Å–æ–≤ –æ–¥–Ω–æ–≤—Ä–µ–º–µ–Ω–Ω–æ\"\n    },\n    \"–∫–∞—á–µ—Å—Ç–≤–æ_—Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–π\": {\n      \"–º–µ—Ç—Ä–∏–∫–∏\": [\n        \"Precision@K - —Ç–æ—á–Ω–æ—Å—Ç—å —Ç–æ–ø-K —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–π\",\n        \"Recall@K - –ø–æ–ª–Ω–æ—Ç–∞ —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–π\",\n        \"NDCG - –Ω–æ—Ä–º–∞–ª–∏–∑–æ–≤–∞–Ω–Ω—ã–π –¥–∏—Å–∫–æ–Ω—Ç–∏—Ä–æ–≤–∞–Ω–Ω—ã–π –∫—É–º—É–ª—è—Ç–∏–≤–Ω—ã–π –≤—ã–∏–≥—Ä—ã—à\",\n        \"MRR - —Å—Ä–µ–¥–Ω–∏–π –æ–±—Ä–∞—Ç–Ω—ã–π —Ä–∞–Ω–≥ –ø–µ—Ä–≤–æ–π —Ä–µ–ª–µ–≤–∞–Ω—Ç–Ω–æ–π —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–∏\"\n      ],\n      \"–º–æ–Ω–∏—Ç–æ—Ä–∏–Ω–≥\": \"–ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–æ–µ –æ—Ç—Å–ª–µ–∂–∏–≤–∞–Ω–∏–µ –ø—Ä–∏–Ω—è—Ç—ã—Ö/–æ—Ç–∫–ª–æ–Ω–µ–Ω–Ω—ã—Ö —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–π\"\n    }\n  },\n  \"–∞–¥–∞–ø—Ç–∏–≤–Ω–æ–µ_–æ–±—É—á–µ–Ω–∏–µ\": {\n    \"–æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ_–≤–µ—Å–æ–≤\": {\n      \"–æ–ø–∏—Å–∞–Ω–∏–µ\": \"–î–∏–Ω–∞–º–∏—á–µ—Å–∫–∞—è –∫–æ—Ä—Ä–µ–∫—Ç–∏—Ä–æ–≤–∫–∞ –≤–µ—Å–æ–≤—ã—Ö –∫–æ—ç—Ñ—Ñ–∏—Ü–∏–µ–Ω—Ç–æ–≤ –Ω–∞ –æ—Å–Ω–æ–≤–µ –æ–±—Ä–∞—Ç–Ω–æ–π —Å–≤—è–∑–∏\",\n      \"–∞–ª–≥–æ—Ä–∏—Ç–º\": \"Gradient descent –Ω–∞ –æ—Å–Ω–æ–≤–µ —É—Å–ø–µ—à–Ω–æ—Å—Ç–∏ —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–π\",\n      \"—á–∞—Å—Ç–æ—Ç–∞\": \"–ï–∂–µ–Ω–µ–¥–µ–ª—å–Ω–æ–µ –æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ –≤–µ—Å–æ–≤\"\n    },\n    \"–ø–µ—Ä—Å–æ–Ω–∞–ª–∏–∑–∞—Ü–∏—è\": {\n      \"–ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—å—Å–∫–∏–µ_–ø—Ä–æ—Ñ–∏–ª–∏\": \"–ò–Ω–¥–∏–≤–∏–¥—É–∞–ª—å–Ω—ã–µ –≤–µ—Å–æ–≤—ã–µ –∫–æ—ç—Ñ—Ñ–∏—Ü–∏–µ–Ω—Ç—ã –¥–ª—è —Ä–∞–∑–Ω—ã—Ö –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª–µ–π\",\n      \"–∏—Å—Ç–æ—Ä–∏—è_–≤–∑–∞–∏–º–æ–¥–µ–π—Å—Ç–≤–∏–π\": \"–£—á–µ—Ç –¥–æ–ª–≥–æ—Å—Ä–æ—á–Ω—ã—Ö –ø—Ä–µ–¥–ø–æ—á—Ç–µ–Ω–∏–π –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è\",\n      \"–∫–ª–∞—Å—Ç–µ—Ä–∏–∑–∞—Ü–∏—è\": \"–ì—Ä—É–ø–ø–∏—Ä–æ–≤–∫–∞ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª–µ–π –ø–æ —Å—Ö–æ–∂–∏–º –ø–∞—Ç—Ç–µ—Ä–Ω–∞–º –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏—è\"\n    }\n  },\n  \"—Ä–µ–∞–ª–∏–∑–∞—Ü–∏—è\": {\n    \"–æ—Å–Ω–æ–≤–Ω—ã–µ_–∫–ª–∞—Å—Å—ã\": {\n      \"TFIDFAnalyzer\": \"–†–∞—Å—á–µ—Ç TF-IDF vectors –¥–ª—è –º–æ–¥—É–ª–µ–π –∏ –∑–∞–ø—Ä–æ—Å–æ–≤\",\n      \"SemanticAnalyzer\": \"–ê–Ω–∞–ª–∏–∑ –Ω–∞–º–µ—Ä–µ–Ω–∏–π –∏ —Å–µ–º–∞–Ω—Ç–∏–∫–∏ –∑–∞–ø—Ä–æ—Å–æ–≤\",\n      \"RecommendationEngine\": \"–û—Å–Ω–æ–≤–Ω–æ–π –¥–≤–∏–∂–æ–∫ —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–π\",\n      \"AdaptiveLearner\": \"–°–∏—Å—Ç–µ–º–∞ –∞–¥–∞–ø—Ç–∏–≤–Ω–æ–≥–æ –æ–±—É—á–µ–Ω–∏—è –≤–µ—Å–æ–≤\"\n    },\n    \"–∏–Ω—Ç–µ–≥—Ä–∞—Ü–∏—è_—Å_cli\": {\n      \"–∫–æ–º–∞–Ω–¥–∞_recommend\": \"slc_cli.py recommend [–∑–∞–ø—Ä–æ—Å] --–∫–æ–ª–∏—á–µ—Å—Ç–≤–æ N\",\n      \"–∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏–µ_–ø—Ä–µ–¥–ª–æ–∂–µ–Ω–∏—è\": \"–ü—Ä–µ–¥–ª–æ–∂–µ–Ω–∏—è –≤–æ –≤—Ä–µ–º—è –ø–æ–∏—Å–∫–∞ –∏ –ø—Ä–æ—Å–º–æ—Ç—Ä–∞\",\n      \"–æ–±—Ä–∞—Ç–Ω–∞—è_—Å–≤—è–∑—å\": \"slc_cli.py feedback [recommendation_id] [–ø–æ–ª–µ–∑–Ω–æ/–Ω–µ–ø–æ–ª–µ–∑–Ω–æ]\"\n    },\n    \"–ø—Ä–æ–∏–∑–≤–æ–¥–∏—Ç–µ–ª—å–Ω–æ—Å—Ç—å\": {\n      \"–≤—Ä–µ–º—è_–æ—Ç–≤–µ—Ç–∞\": \"< 200–º—Å –¥–ª—è –≥–µ–Ω–µ—Ä–∞—Ü–∏–∏ —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–π\",\n      \"–∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏–µ_–ø–∞–º—è—Ç–∏\": \"< 50–ú–ë –¥–ª—è –∫—ç—à–∏—Ä–æ–≤–∞–Ω–Ω—ã—Ö –¥–∞–Ω–Ω—ã—Ö\",\n      \"–≤–ª–∏—è–Ω–∏–µ_–Ω–∞_cli\": \"< 2% –∑–∞–º–µ–¥–ª–µ–Ω–∏—è –æ—Å–Ω–æ–≤–Ω—ã—Ö –æ–ø–µ—Ä–∞—Ü–∏–π\"\n    }\n  },\n  \"–ø–ª–∞–Ω_—Ä–µ–∞–ª–∏–∑–∞—Ü–∏–∏\": {\n    \"—ç—Ç–∞–ø_1_–±–∞–∑–æ–≤—ã–π_tf_idf\": [\n      \"‚òê –°–æ–∑–¥–∞–Ω–∏–µ TFIDFAnalyzer –∫–ª–∞—Å—Å–∞\",\n      \"‚òê –ü–æ—Å—Ç—Ä–æ–µ–Ω–∏–µ —Å–ª–æ–≤–∞—Ä—è —Ç–µ—Ä–º–∏–Ω–æ–≤ –∏–∑ –º–æ–¥—É–ª–µ–π\",\n      \"‚òê –†–∞—Å—á–µ—Ç IDF –¥–ª—è —Ç–µ—Ä–º–∏–Ω–æ–≤\",\n      \"‚òê –§—É–Ω–∫—Ü–∏—è —Ä–∞—Å—á–µ—Ç–∞ TF-IDF –¥–ª—è –∑–∞–ø—Ä–æ—Å–æ–≤\"\n    ],\n    \"—ç—Ç–∞–ø_2_—Å–µ–º–∞–Ω—Ç–∏—á–µ—Å–∫–∏–π_–∞–Ω–∞–ª–∏–∑\": [\n      \"‚òê SemanticAnalyzer –¥–ª—è –∞–Ω–∞–ª–∏–∑–∞ –Ω–∞–º–µ—Ä–µ–Ω–∏–π\",\n      \"‚òê –ò–∑–≤–ª–µ—á–µ–Ω–∏–µ –∫–ª—é—á–µ–≤—ã—Ö —Ç–µ—Ä–º–∏–Ω–æ–≤ –∏–∑ –∑–∞–ø—Ä–æ—Å–æ–≤\",\n      \"‚òê –û–ø—Ä–µ–¥–µ–ª–µ–Ω–∏–µ –∫–æ–Ω—Ç–µ–∫—Å—Ç—É–∞–ª—å–Ω—ã—Ö –ø–æ–¥—Å–∫–∞–∑–æ–∫\",\n      \"‚òê –ò–Ω—Ç–µ–≥—Ä–∞—Ü–∏—è —Å –∫–∞—Ç–µ–≥–æ—Ä–∏—è–º–∏ –Ω–∞–º–µ—Ä–µ–Ω–∏–π\"\n    ],\n    \"—ç—Ç–∞–ø_3_—Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ç–µ–ª—å–Ω—ã–π_–¥–≤–∏–∂–æ–∫\": [\n      \"‚òê RecommendationEngine –æ—Å–Ω–æ–≤–Ω–æ–π –∫–ª–∞—Å—Å\",\n      \"‚òê –ö–æ–º–±–∏–Ω–∏—Ä–æ–≤–∞–Ω–∏–µ –≤—Å–µ—Ö —Ñ–∞–∫—Ç–æ—Ä–æ–≤ –æ—Ü–µ–Ω–∫–∏\",\n      \"‚òê –†–∞–Ω–∂–∏—Ä–æ–≤–∞–Ω–∏–µ –∏ —Ñ–∏–ª—å—Ç—Ä–∞—Ü–∏—è —Ä–µ–∑—É–ª—å—Ç–∞—Ç–æ–≤\",\n      \"‚òê –ò–Ω—Ç–µ–≥—Ä–∞—Ü–∏—è —Å UsageTracker –¥–∞–Ω–Ω—ã–º–∏\"\n    ],\n    \"—ç—Ç–∞–ø_4_–∏–Ω—Ç–µ–≥—Ä–∞—Ü–∏—è_–∏_–æ–ø—Ç–∏–º–∏–∑–∞—Ü–∏—è\": [\n      \"‚òê –ò–Ω—Ç–µ–≥—Ä–∞—Ü–∏—è –≤ slc_cli.py\",\n      \"‚òê –ö—ç—à–∏—Ä–æ–≤–∞–Ω–∏–µ –∏ –æ–ø—Ç–∏–º–∏–∑–∞—Ü–∏—è –ø—Ä–æ–∏–∑–≤–æ–¥–∏—Ç–µ–ª—å–Ω–æ—Å—Ç–∏\",\n      \"‚òê –°–∏—Å—Ç–µ–º–∞ –æ–±—Ä–∞—Ç–Ω–æ–π —Å–≤—è–∑–∏\",\n      \"‚òê –¢–µ—Å—Ç–∏—Ä–æ–≤–∞–Ω–∏–µ –∏ –≤–∞–ª–∏–¥–∞—Ü–∏—è –∫–∞—á–µ—Å—Ç–≤–∞\"\n    ]\n  },\n  \"—Ç–µ—Å—Ç–∏—Ä–æ–≤–∞–Ω–∏–µ\": {\n    \"unit_tests\": \"–¢–µ—Å—Ç–∏—Ä–æ–≤–∞–Ω–∏–µ –∫–∞–∂–¥–æ–≥–æ –∞–ª–≥–æ—Ä–∏—Ç–º–∞ –æ—Ç–¥–µ–ª—å–Ω–æ\",\n    \"integration_tests\": \"–¢–µ—Å—Ç–∏—Ä–æ–≤–∞–Ω–∏–µ –ø–æ–ª–Ω–æ–≥–æ –ø–∞–π–ø–ª–∞–π–Ω–∞ —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–π\",\n    \"performance_tests\": \"–ü—Ä–æ–≤–µ—Ä–∫–∞ —Å–∫–æ—Ä–æ—Å—Ç–∏ –∏ –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏—è —Ä–µ—Å—É—Ä—Å–æ–≤\",\n    \"accuracy_tests\": \"–ü—Ä–æ–≤–µ—Ä–∫–∞ –∫–∞—á–µ—Å—Ç–≤–∞ —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–π –Ω–∞ —Ç–µ—Å—Ç–æ–≤—ã—Ö –¥–∞–Ω–Ω—ã—Ö\"\n  },\n  \"–∫—Ä–∏—Ç–µ—Ä–∏–∏_—É—Å–ø–µ—Ö–∞\": {\n    \"—Ç–æ—á–Ω–æ—Å—Ç—å_—Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–π\": \"> 80% —Ä–µ–ª–µ–≤–∞–Ω—Ç–Ω—ã—Ö –ø—Ä–µ–¥–ª–æ–∂–µ–Ω–∏–π –≤ —Ç–æ–ø-5\",\n    \"—Å–∫–æ—Ä–æ—Å—Ç—å_–≥–µ–Ω–µ—Ä–∞—Ü–∏–∏\": \"< 200–º—Å –¥–ª—è –ø–æ–ª—É—á–µ–Ω–∏—è —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–π\",\n    \"–ø–æ–∫—Ä—ã—Ç–∏–µ_–º–æ–¥—É–ª–µ–π\": \"> 90% –º–æ–¥—É–ª–µ–π –¥–æ—Å—Ç—É–ø–Ω—ã –¥–ª—è —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–π\",\n    \"–ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—å—Å–∫–æ–µ_–ø—Ä–∏–Ω—è—Ç–∏–µ\": \"> 70% —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–π –∏—Å–ø–æ–ª—
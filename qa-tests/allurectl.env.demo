# Demo Configuration for Allure TestOps
# Это демо конфигурация для тестирования интеграции

# TestOps Endpoint (демо - замените на реальный)
export ALLURE_ENDPOINT=https://demo.testops.example.com

# API Token (демо - замените на реальный)
export ALLURE_TOKEN=demo-token-12345

# Project ID (демо - замените на реальный)
export ALLURE_PROJECT_ID=100

# Launch Name
export ALLURE_LAUNCH_NAME="Cellframe Node QA Demo - $(date +%Y-%m-%d_%H-%M)"

# Launch Tags
export ALLURE_LAUNCH_TAGS="cellframe,node,qa,demo,test"

# Launch Description
export ALLURE_LAUNCH_DESCRIPTION="Demo integration of Cellframe Node QA with Allure TestOps"

# Environment
export ALLURE_ENVIRONMENT="demo"

# Build Name
export ALLURE_BUILD_NAME="cellframe-node-demo-$(date +%Y%m%d-%H%M%S)"




